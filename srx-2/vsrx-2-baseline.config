system {
    host-name vSRX-2;
    root-authentication {
        encrypted-password "$1$rUleI7B.$lzOAmvzh2qAcDdLXri2Cb/"; ## SECRET-DATA
        ssh-dsa "ssh-dss  configurator@server1.he"; ## SECRET-DATA
    }
    login {
        user lab {
            uid 2000;
            class super-user;
            authentication {
                encrypted-password "$1$84J5Maes$cni5Hrazbd/IEHr/50oY30"; ## SECRET-DATA
            }
        }
    }
    services {
        ftp;
        ssh;
        telnet;
        web-management {
            http {
                interface fxp0.0;
